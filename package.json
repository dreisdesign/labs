{
  "name": "labs-root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dreisdesign/labs.git"
  },
  "scripts": {
    "check-storybook-theme": "node scripts/check-storybook-theme.js",
    "public": "npm --prefix design-system run public",
    "local": "npm --prefix design-system run local && (sleep 2 && open -a 'Google Chrome' http://localhost:8080/docs/demo/index.html &) && python3 -m http.server 8080",
    "github": "npm --prefix design-system run github",
    "token-scan": "node scripts/token-scan-node.js && ./scripts/token-report.sh",
    "deploy": "bash scripts/deploy.sh",
    "sync-icons": "node scripts/sync-icons.js",
    "build-storybook": "npm --prefix design-system run build-storybook",
    "private": "npm --prefix design-system run local && (sleep 2 && open -a 'Google Chrome' http://localhost:8080/docs/demo/index.html &) && python3 -m http.server 8080",
    "storybook": "npm --prefix design-system run storybook",
    "premenu": "node scripts/generate-icons-list.mjs",
    "menu": "node scripts/menu.mjs",
    "s": "cd /Users/danielreis/labs && echo '1' | npm run menu",
    "smoothie": "cd /Users/danielreis/labs && echo '1' | npm run menu",
    "labs": "cd /Users/danielreis/labs && echo '2' | npm run menu",
    "l": "cd /Users/danielreis/labs && echo '2' | npm run menu",
    "r": "cd /Users/danielreis/labs && echo '3' | npm run menu",
    "d": "cd /Users/danielreis/labs && echo '4' | npm run menu",
    "u": "cd /Users/danielreis/labs && echo '5' | npm run menu"
  },
  "devDependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "inquirer": "^12.8.2",
    "svgo": "^4.0.0"
  },
  "dependencies": {
    "puppeteer": "^24.16.2"
  }
}