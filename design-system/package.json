{
  "name": "design-system",
  "version": "2.1.5",
  "description": "Labs Design System v2 - Complete theme system with modular components, Storybook v9.1.2, and ready-to-use button configurations",
  "main": "index.js",
  "scripts": {
    "generate-icons-list": "node ../scripts/generate-icons-list.mjs",
    "prestorybook": "npm run test:unit && npm run generate-icons-list && node ../scripts/sync-icons.js",
    "storybook": "bash scripts/start-storybook.sh",
    "storybook-original": "storybook dev -p 6006",
    "prebuild-storybook": "npm run generate-icons-list && node ../scripts/sync-icons.js",
    "build-storybook": "NODE_ENV=production bash scripts/build-storybook.sh",
    "build-storybook-original": "NODE_ENV=production storybook build",
    "build": "node ../scripts/update-static-paths.js --auto",
    "public": "node ../scripts/update-static-paths.js --auto",
    "local": "node scripts/open-storybook-local.js",
    "github": "node ../scripts/update-static-paths.js --auto",
    "lint:tokens:rgb": "node ../scripts/lint-rgb.js",
    "test:unit": "vitest --config ./vitest.node.config.js run"
  },
  "keywords": [
    "storybook",
    "design-system",
    "web-components"
  ],
  "author": "danielreis",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-themes": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/web-components-vite": "^10.0.8",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^21.1.0",
    "lit": "^3.3.1",
    "playwright": "^1.54.1",
    "storybook": "^10.0.8",
    "vitest": "^3.2.4"
  }
}
